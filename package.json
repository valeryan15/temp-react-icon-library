{
  "name": "@valeryan15/temp-react-icon-library",
  "version": "1.0.3",
  "description": "A modern, performant React icon library with TypeScript support",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./icons/*": {
      "import": "./dist/icons/*.mjs",
      "require": "./dist/icons/*.cjs",
      "types": "./dist/icons/*.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf dist && mkdir dist && vite build && vite build --config vite.icons.config.ts",
    "dev": "vite",
    "test": "jest --config=jest.config.cjs",
    "test:watch": "jest --config=jest.config.cjs --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "generate-icons": "node scripts/generate-icons.cjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build",
    "prepare": "husky",
    "publish-package": "node scripts/publish-package.cjs"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "jest --config=jest.config.cjs --findRelatedTests"
    ]
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "typescript",
    "components",
    "ui"
  ],
  "author": "Valerii Malanin",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.14.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/blocks": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/react-vite": "^7.6.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.3",
    "svgo": "^3.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/valeryan15/temp-react-icon-library.git"
  },
  "bugs": {
    "url": "https://github.com/valeryan15/temp-react-icon-library/issues"
  },
  "homepage": "https://github.com/valeryan15/temp-react-icon-library#readme"
}
